{
  "name": "ZFAdSdk",
  "version": "1.0.0",
  "summary": "A short summary of ZFAdSdk.",
  "description": "A short description of ZFAdSdk.",
  "homepage": "https://github.com/Havarry/ZFAdSdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Havarry": "15622308894@163.com"
  },
  "source": {
    "git": "https://github.com/Havarry/ZFAdSdk.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "static_framework": true,
  "subspecs": [
    {
      "name": "ZFCore",
      "source_files": "ZFAdSdk/Classes/**/*",
      "dependencies": {
        "SVProgressHUD": [

        ],
        "FFToast": [

        ]
      },
      "frameworks": [
        "AdSupport",
        "CoreData",
        "SystemConfiguration",
        "AVFoundation",
        "CoreMedia"
      ]
    },
    {
      "name": "admob_sdk",
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "7.68.0"
        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) ADMOB_ADS_ENABLED"
      }
    },
    {
      "name": "byte_dance_ads_sdk",
      "dependencies": {
        "Bytedance-UnionAD": [
          "3.3.6.2"
        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) BYTE_DANCE_ADS_ENABLED"
      }
    },
    {
      "name": "gdt_ads_sdk",
      "dependencies": {
        "GDTMobSDK": [
          "4.12.1"
        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GDT_ADS_ENABLED"
      }
    }
  ]
}
